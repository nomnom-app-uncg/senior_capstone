{
  "expo": {
    "name": "prototype-app",
    "slug": "prototype-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.bmohamad.prototypeapp",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to capture photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA"],
      "package": "com.bmohamad.prototypeapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ".src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": ".src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "71b76e37-164e-481c-9315-e7033bf48337"
      }
    }
  }
}
